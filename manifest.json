{
	"manifest_version": 2,
	"name": "MireaJacked",
	"version": "1.0.3",
	"description": "Возвращает возможность копирование, выделение и вставки в тестах МИЭРА",
	"icons": {
		"96": "icon96.png"
	},
	"permissions": [
		"activeTab",
		"storage",
		"clipboardRead", 
		"clipboardWrite"
	],
	"content_scripts": [
		{
			"matches": [
				"*://online-edu.mirea.ru/mod/quiz/*"
			],
			"js": [
				"main.js"
			]
		}
	]
}